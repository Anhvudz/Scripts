PresetSection:NewButton({
    Name = "Small (5x5x5)",
    Callback = function()
        hitboxSizeX, hitboxSizeY, hitboxSizeZ = 5, 5, 5
        MyHub:Notify("Preset", "Small hitbox selected", 2)
    end,
})

